<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="customised-Disability" path="/I2CE">
  <metadata>
    <version>4.1.4</version>
  </metadata>
  <configurationGroup name="customised-Disability" path="/I2CE" locale="en_US">
    <version>4.1.4</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="en_US">
      <configuration name="can_edit_database_list_disability" locale="en_US">
        <value>Can edit disability list</value>
      </configuration>
      <configuration name="can_view_database_list_disability" locale="en_US">
        <value>Can view disability list</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms/">
      <configurationGroup name="formClasses">
        <configurationGroup name="CUSTOMISED_Disability">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Name (Blind, etc.)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="en_US">
                <value>default:Code (1, 2, ...)</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>
