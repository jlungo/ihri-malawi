<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-PersonPosition" path="/I2CE">
  <metadata>
    <version>4.1.9.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-PersonPosition" path="/I2CE" locale="et">
    <version>4.1.9.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="et">
      <configuration name="pp_bad_date" path="person_position/start_date/bad_date"></configuration>
      <configuration name="pp_end_date" path="person_position/end_date/bad_date"></configuration>
      <configuration name="pp_end_date" path="person_position/end_date/required"></configuration>
      <configuration name="pp_reason" path="person_position/reason/required"></configuration>
      <configuration name="p_status" path="position/status/required"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_department" locale="et">
        <value>Redigeeri osakondade nimekirja</value>
      </configuration>
      <configuration name="can_view_database_list_department" locale="et">
        <value>Vaata osakondade nimekirja</value>
      </configuration>
      <configuration name="can_edit_database_list_position_type" locale="et">
        <value>Redigeeri positsiooni_tüüp nimekirja</value>
      </configuration>
      <configuration name="can_view_database_list_position_type" locale="et">
        <value>Vaata positsiooni_tüüp nimekirja</value>
      </configuration>
      <configuration name="can_edit_database_list_position" locale="et">
        <value>Redigeeri positsioonide nimekirja</value>
      </configuration>
      <configuration name="can_view_database_list_position" locale="et">
        <value>Vaata positsioonide nimekirja</value>
      </configuration>
      <configuration name="can_edit_database_list_pos_change_reason" locale="et">
        <value>Redigeeri pos_muutmise_põhjus nimekirja</value>
      </configuration>
      <configuration name="can_view_database_list_pos_change_reason" locale="et">
        <value>Vaata pos_muutmise_põhjus nimekirja</value>
      </configuration>
      <configuration name="can_edit_manage_positions_database_lists" locale="et">
        <value>Saab redigeerida mõnd informatsiooni positsioonide kohta</value>
      </configuration>
      <configuration name="can_view_manage_positions_database_lists" locale="et">
        <value>Saab vaadata mõnd informatsiooni positsioonide kohta</value>
      </configuration>
      <configuration name="can_edit_all_manage_positions_database_lists" locale="et">
        <value>Saab redigeerida kogu informatsiooni positsioonide kohta</value>
      </configuration>
      <configuration name="can_view_all_manage_positions_database_lists" locale="et">
        <value>Saab vaadata kogu informatsiooni positsioonide kohta</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_position" locale="et">
        <value>Saab redigeerida isiku positsiooni</value>
      </configuration>
      <configuration name="person_can_view_child_form_person_position" locale="et">
        <value>Saab vaadata isiku positsiooni</value>
      </configuration>
      <configuration name="person_can_change_child_form_person_position" locale="et">
        <value>Saab muuta isiku olemas olevat positsiooni</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="position_status">
        <configurationGroup name="open">
          <configuration name="fields" value="many" type="delimited" locale="et">
            <value>name:Ava</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="closed">
          <configuration name="fields" value="many" type="delimited" locale="et">
            <value>name:Suletud</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="discontinued">
          <configuration name="fields" value="many" type="delimited" locale="et">
            <value>name:Peatatud</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="departure">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Märgi lahkumine</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="make_offer">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Tee tööpakkumine</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Redigeeri kohta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="et">
            <value>Vaata kohta</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_position">
          <configuration name="display" values="single" locale="et">
            <value>Isiku positsioon</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="et">
              <value>Seda vormi kasutatakse, et ühendada isik teatud positsiooni asukohaga</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="pos_change_reason">
          <configuration name="display" values="single" locale="et">
            <value>Lahkumise põhjused</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_status">
          <configuration name="display" values="single" locale="et">
            <value>Positsiooni olek</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="display" values="single" locale="et">
            <value>Positsioon</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_type">
          <configuration name="display" values="single" locale="et">
            <value>Positsioonitüüp</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="department">
          <configuration name="display" values="single" locale="et">
            <value>Osakond</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Department">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Nimi</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PersonPosition">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsioon</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Alguskuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Lõppkuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Põhjus lahkumiseks</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsiooni kood</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Töökoht</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsiooni tiitel</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsiooni seletus</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Ülevaataja</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Jaoskond</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="department">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Osakond</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="pos_type">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsioonitüüp</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="proposed_hiring_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Pakutav tööle võtmise kuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="posted_date">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Postituse kuupäev</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="status">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Olek</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Positsiooni kommentaarid</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="interview_comments">
              <configuration name="headers" type="delimited" locale="et">
                <value>default:Intervjuu kommentaarid</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>