            <value>name:فتح</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="closed">
          <configuration name="fields" value="many" type="delimited" locale="ar">
            <value>name:مغلق</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="discontinued">
          <configuration name="fields" value="many" type="delimited" locale="ar">
            <value>name:موقوف</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
