<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-PersonPosition" path="/I2CE">
  <metadata>
    <version>4.1.9.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-PersonPosition" path="/I2CE" locale="pt_BR">
    <version>4.1.9.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="pt_BR">
      <configuration name="pp_bad_date" path="person_position/start_date/bad_date">
        <value>Por favor, forneça uma data inicial após a posição anterior.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/bad_date">
        <value>A data final precisa estar após a data inicial.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/required">
        <value>Campo necessário</value>
      </configuration>
      <configuration name="pp_reason" path="person_position/reason/required">
        <value>Campo necessário</value>
      </configuration>
      <configuration name="p_status" path="position/status/required">
        <value>Por favor, forneça um novo estado.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_department" locale="pt_BR">
        <value>Editar a lista de departamentos</value>
      </configuration>
      <configuration name="can_view_database_list_department" locale="pt_BR">
        <value>Ver a lista de departamentos</value>
      </configuration>
      <configuration name="can_edit_database_list_position_type" locale="pt_BR">
        <value>Editar a lista tipo_de_cargo</value>
      </configuration>
      <configuration name="can_view_database_list_position_type" locale="pt_BR">
        <value>Ver a lista tipo_de_cargo</value>
      </configuration>
      <configuration name="can_edit_database_list_position" locale="pt_BR">
        <value>Editar a lista de cargos</value>
      </configuration>
      <configuration name="can_view_database_list_position" locale="pt_BR">
        <value>Ver a lista de cargo</value>
      </configuration>
      <configuration name="can_edit_database_list_pos_change_reason" locale="pt_BR">
        <value>Editar a lista mudar_razao_pos</value>
      </configuration>
      <configuration name="can_view_database_list_pos_change_reason" locale="pt_BR">
        <value>Ver a lista mudar_razao_pos</value>
      </configuration>
      <configuration name="can_edit_manage_positions_database_lists" locale="pt_BR">
        <value>Pode editar algumas informações sobre cargos</value>
      </configuration>
      <configuration name="can_view_manage_positions_database_lists" locale="pt_BR">
        <value>Pode ver algumas informações sobre cargos</value>
      </configuration>
      <configuration name="can_edit_all_manage_positions_database_lists" locale="pt_BR">
        <value>Pode editar todas as informações sobre cargos</value>
      </configuration>
      <configuration name="can_view_all_manage_positions_database_lists" locale="pt_BR">
        <value>Pode ver todas as informações sobre cargos</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_position" locale="pt_BR">
        <value>Pode editar o cargo de uma pessoa</value>
      </configuration>
      <configuration name="person_can_view_child_form_person_position" locale="pt_BR">
        <value>Pode ver o cargo de uma pessoa</value>
      </configuration>
      <configuration name="person_can_change_child_form_person_position" locale="pt_BR">
        <value>Pode mudar o a existência do cargo de uma pessoa</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="position_status">
        <configurationGroup name="open">
          <configuration name="fields" value="many" type="delimited" locale="pt_BR">
            <value>name:Abrir</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="closed">
          <configuration name="fields" value="many" type="delimited" locale="pt_BR">
            <value>name:Fechado</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="discontinued">
          <configuration name="fields" value="many" type="delimited" locale="pt_BR">
            <value>name:Descontinuado</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="departure">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Gravar uma Demissão</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="make_offer">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Fazer Oferta de Trabalho</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Editar Cargo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR">
            <value>Ver Cargo</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_position">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Cargo da Pessoa</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="pt_BR">
              <value>Esse formulário é usado para ligar uma pessoa a um cargo particular residencial</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="pos_change_reason">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Razões para Demissão</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_status">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Status do Cargo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Cargo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_type">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Tipo de Cargo</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="department">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Departamento</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Department">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Nome</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PersonPosition">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Cargo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data de Início</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data de Término</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Razão para Demissão</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Código do Cargo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Emprego</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Nome do Cargo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Descrição do Cargo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="pt_BR"></configuration>
            </configurationGroup>
            <configurationGroup name="facility">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Escritório</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="department">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Departamento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="pos_type">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Tipo de Cargo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="proposed_hiring_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data de Admissão Proposta</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="posted_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data do Envio</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="status">
              <configuration name="headers" type="delimited" locale="pt_BR"></configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Comentários do Cargo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="interview_comments">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Comentários da Entrevista</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>