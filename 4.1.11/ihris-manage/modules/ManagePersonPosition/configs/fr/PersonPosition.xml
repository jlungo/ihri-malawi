<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-PersonPosition" path="/I2CE">
  <metadata>
    <version>4.1.9.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-PersonPosition" path="/I2CE" locale="fr">
    <version>4.1.9.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="fr">
      <configuration name="pp_bad_date" path="person_position/start_date/bad_date">
        <value>Veuillez saisir une date de début postérieure à la date de début de la position précédente.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/bad_date">
        <value>La date de fin doit être postérieure à la date de début.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/required">
        <value>Champ obligatoire.</value>
      </configuration>
      <configuration name="pp_reason" path="person_position/reason/required">
        <value>Champ obligatoire.</value>
      </configuration>
      <configuration name="p_status" path="position/status/required">
        <value>Veuillez entrer un nouvel état.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_department" locale="fr">
        <value>Editer la liste des départements</value>
      </configuration>
      <configuration name="can_view_database_list_department" locale="fr">
        <value>Afficher la liste des départements</value>
      </configuration>
      <configuration name="can_edit_database_list_position_type" locale="fr">
        <value>Editer la liste des types de poste</value>
      </configuration>
      <configuration name="can_view_database_list_position_type" locale="fr">
        <value>Afficher la liste des types de poste</value>
      </configuration>
      <configuration name="can_edit_database_list_position" locale="fr">
        <value>Editer la liste des postes</value>
      </configuration>
      <configuration name="can_view_database_list_position" locale="fr">
        <value>Afficher la liste des postes</value>
      </configuration>
      <configuration name="can_edit_database_list_pos_change_reason" locale="fr">
        <value>Editer la liste des raisons des changements de poste</value>
      </configuration>
      <configuration name="can_view_database_list_pos_change_reason" locale="fr">
        <value>Afficher la liste des raisons des changements de poste</value>
      </configuration>
      <configuration name="can_edit_manage_positions_database_lists" locale="fr">
        <value>Peut éditer certaines informations sur les postes</value>
      </configuration>
      <configuration name="can_view_manage_positions_database_lists" locale="fr">
        <value>Peut afficher certaines informations sur les postes</value>
      </configuration>
      <configuration name="can_edit_all_manage_positions_database_lists" locale="fr">
        <value>Peut éditer toutes les informations sur les postes</value>
      </configuration>
      <configuration name="can_view_all_manage_positions_database_lists" locale="fr">
        <value>Peut afficher toutes les informations sur les postes</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_position" locale="fr">
        <value>Peut éditer le poste d’une personne</value>
      </configuration>
      <configuration name="person_can_view_child_form_person_position" locale="fr">
        <value>Peut afficher le poste d’une personne</value>
      </configuration>
      <configuration name="person_can_change_child_form_person_position" locale="fr">
        <value>Peut changer le poste existant d’une personne</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="position_status">
        <configurationGroup name="open">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Vacant</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="closed">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Clôturé</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="discontinued">
          <configuration name="fields" value="many" type="delimited" locale="fr">
            <value>name:Arrêté</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="departure">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Enregistrer un départ</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="make_offer">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Faire une offre d’emploi</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Editer un poste</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Afficher un poste</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_position">
          <configuration name="display" values="single" locale="fr">
            <value>Poste d'une personne</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="fr">
              <value>Ce formulaire est utilisé pour lier une personne à une résidence d'un poste particulier</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="pos_change_reason">
          <configuration name="display" values="single" locale="fr">
            <value>Raisons du départ</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_status">
          <configuration name="display" values="single" locale="fr">
            <value>Statut du poste</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="display" values="single" locale="fr">
            <value>Poste</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_type">
          <configuration name="display" values="single" locale="fr">
            <value>Type de Poste</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="department">
          <configuration name="display" values="single" locale="fr">
            <value>Service</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Department">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Nom</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PersonPosition">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de début</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de fin</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Raison du départ</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Code du poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Emploi</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Intitulé du poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Description du poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Superviseur</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Établissement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="department">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Service</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="pos_type">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Type de Poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="proposed_hiring_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date proposée pour l’embauche</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="posted_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date de publication</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="status">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Statut</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Commentaires sur le poste</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="interview_comments">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Commentaires sur l’entretien</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>