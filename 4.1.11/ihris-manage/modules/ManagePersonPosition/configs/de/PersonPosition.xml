<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-PersonPosition" path="/I2CE">
  <metadata>
    <version>4.1.9.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-PersonPosition" path="/I2CE" locale="de">
    <version>4.1.9.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="de">
      <configuration name="pp_bad_date" path="person_position/start_date/bad_date">
        <value>Bitte gib ein Anfangsdatum nach dem Anfangsdatum deiner vorherigen Stelle an.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/bad_date">
        <value>Das Enddatum muss nach dem Anfangsdatum liegen.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/required">
        <value>Pflichtfeld</value>
      </configuration>
      <configuration name="pp_reason" path="person_position/reason/required">
        <value>Pflichtfeld</value>
      </configuration>
      <configuration name="p_status" path="position/status/required">
        <value>Bitte gib einen neuen Status an.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_department" locale="de">
        <value>Liste der Abteilungen bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_department" locale="de">
        <value>Liste der Abteilungen einsehen</value>
      </configuration>
      <configuration name="can_edit_database_list_position_type" locale="de">
        <value>Liste der Stellenarten bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_position_type" locale="de">
        <value>Liste der Stellenarten einsehen</value>
      </configuration>
      <configuration name="can_edit_database_list_position" locale="de">
        <value>Liste der Stellen bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_position" locale="de">
        <value>Liste der Stellen einsehen</value>
      </configuration>
      <configuration name="can_edit_database_list_pos_change_reason" locale="de">
        <value>Listen der Gründe für Stellenumbesetzungen bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_pos_change_reason" locale="de">
        <value>Listen der Gründe für Stellenumbesetzungen einsehen</value>
      </configuration>
      <configuration name="can_edit_manage_positions_database_lists" locale="de">
        <value>Kann Informationen zu Stellen bearbeiten</value>
      </configuration>
      <configuration name="can_view_manage_positions_database_lists" locale="de">
        <value>Kann Informationen zu Stellen einsehen</value>
      </configuration>
      <configuration name="can_edit_all_manage_positions_database_lists" locale="de">
        <value>Kann alle Informationen zu Stellen bearbeiten</value>
      </configuration>
      <configuration name="can_view_all_manage_positions_database_lists" locale="de">
        <value>Kann alle Informationen zu Stellen einsehen</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_position" locale="de">
        <value>Kann die Stellen eines Mitarbeiters bearbeiten</value>
      </configuration>
      <configuration name="person_can_view_child_form_person_position" locale="de">
        <value>Kann die Stellen eines Mitarbeiters einsehen</value>
      </configuration>
      <configuration name="person_can_change_child_form_person_position" locale="de">
        <value>Kann die bestehende Stelle eines Mitarbeiters ändern</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="position_status">
        <configurationGroup name="open">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Offen</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="closed">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Geschlossen</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="discontinued">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Ausgelaufen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="departure">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Weggang aufzeichnen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="make_offer">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Ein Arbeitsangebot abgeben</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Stelle bearbeiten</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Stelle einsehen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_position">
          <configuration name="display" values="single" locale="de">
            <value>Stelle des Mitarbeiters</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="de">
              <value>Dieses Formular wird dazu verwendet, um einen Mitarbeiter einer bestimmten Arbeitsunterkunft zuzuweisen</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="pos_change_reason">
          <configuration name="display" values="single" locale="de">
            <value>Gründe für den Weggang</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_status">
          <configuration name="display" values="single" locale="de">
            <value>Status der Stelle</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="display" values="single" locale="de">
            <value>Stelle</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_type">
          <configuration name="display" values="single" locale="de">
            <value>Stellenart</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="department">
          <configuration name="display" values="single" locale="de">
            <value>Abteilung</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Department">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PersonPosition">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Stelle</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anfangsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Enddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Gründe für den Weggang</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Stellennummer</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Tätigkeit</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Stellenbezeichnung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Stellenbeschreibung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Vorgesetzter</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Einrichtung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="department">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Abteilung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="pos_type">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Stellenart</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="proposed_hiring_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Vorgeschlagenes Einstellungsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="posted_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Beitragsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="status">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Bemerkungen zur Stelle</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="interview_comments">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Bemerkungen zum Vorstellungsgespräch</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>