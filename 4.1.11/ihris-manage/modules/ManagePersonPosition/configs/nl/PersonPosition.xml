<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-manage-PersonPosition" path="/I2CE">
  <metadata>
    <version>4.1.9.0.1</version>
  </metadata>
  <configurationGroup name="ihris-manage-PersonPosition" path="/I2CE" locale="nl">
    <version>4.1.9.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="nl">
      <configuration name="pp_bad_date" path="person_position/start_date/bad_date">
        <value>Geef een begindatum in na de begindatum van de vorige positie.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/bad_date">
        <value>De einddatum moet na de begindatum liggen.</value>
      </configuration>
      <configuration name="pp_end_date" path="person_position/end_date/required">
        <value>Verplicht veld</value>
      </configuration>
      <configuration name="pp_reason" path="person_position/reason/required">
        <value>Verplicht veld</value>
      </configuration>
      <configuration name="p_status" path="position/status/required">
        <value>Geef een nieuwe status op.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description">
      <configuration name="can_edit_database_list_department" locale="nl">
        <value>Afdelingenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_department" locale="nl">
        <value>Afdelingenlijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_position_type" locale="nl">
        <value>Positietypenlijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_position_type" locale="nl">
        <value>Positietypenlijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_position" locale="nl">
        <value>Positielijst bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_position" locale="nl">
        <value>Positielijst bekijken</value>
      </configuration>
      <configuration name="can_edit_database_list_pos_change_reason" locale="nl">
        <value>Lijst met redenen voor positiewijziging bewerken</value>
      </configuration>
      <configuration name="can_view_database_list_pos_change_reason" locale="nl">
        <value>Lijst met redenen voor positiewijziging bekijken</value>
      </configuration>
      <configuration name="can_edit_manage_positions_database_lists" locale="nl">
        <value>Kan enige gegevens over posities bewerken</value>
      </configuration>
      <configuration name="can_view_manage_positions_database_lists" locale="nl">
        <value>Kan enige gegevens over posities bekijken</value>
      </configuration>
      <configuration name="can_edit_all_manage_positions_database_lists" locale="nl">
        <value>Kan alle gegevens over posities bewerken</value>
      </configuration>
      <configuration name="can_view_all_manage_positions_database_lists" locale="nl">
        <value>Kan alle gegevens over posities bekijken</value>
      </configuration>
      <configuration name="person_can_edit_child_form_person_position" locale="nl">
        <value>Kan de positie van een persoon bewerken</value>
      </configuration>
      <configuration name="person_can_view_child_form_person_position" locale="nl">
        <value>Kan de positie van een persoon bekijken</value>
      </configuration>
      <configuration name="person_can_change_child_form_person_position" locale="nl">
        <value>Kan de huidige positie van een persoon wijzigen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="position_status">
        <configurationGroup name="open">
          <configuration name="fields" value="many" type="delimited" locale="nl"></configuration>
        </configurationGroup>
        <configurationGroup name="closed">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Gesloten</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="discontinued">
          <configuration name="fields" value="many" type="delimited" locale="nl">
            <value>name:Niet verlengd</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="departure">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Vertrek registreren</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="make_offer">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Een functieaanbod doen</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="person_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Positie bewerken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_position">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Positie bekijken</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_position">
          <configuration name="display" values="single" locale="nl">
            <value>Persoonpositie</value>
          </configuration>
          <configurationGroup name="meta">
            <configuration name="description" locale="nl">
              <value>Dit formulier wordt gebruikt om een persoon te koppelen aan een bepaalde vestigingsplaats voor een positie</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="pos_change_reason">
          <configuration name="display" values="single" locale="nl">
            <value>Redenen voor vertrek</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_status">
          <configuration name="display" values="single" locale="nl">
            <value>Positiestatus</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position">
          <configuration name="display" values="single" locale="nl">
            <value>Positie</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="position_type">
          <configuration name="display" values="single" locale="nl">
            <value>Positietype</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="department">
          <configuration name="display" values="single" locale="nl">
            <value>Afdeling</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Department">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Naam</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_PersonPosition">
          <configurationGroup name="fields">
            <configurationGroup name="position">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Positie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Begindatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Einddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="reason">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Reden voor vertrek</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Position">
          <configurationGroup name="fields">
            <configurationGroup name="code">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Positiecode</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Functie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="title">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Positienaam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Positiebeschrijving</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="supervisor">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Leidinggevende</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="facility">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Instelling</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="department">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Afdeling</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="pos_type">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Positietype</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="proposed_hiring_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Voorgestelde aannamedatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="posted_date">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Datum plaatsing</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="status">
              <configuration name="headers" type="delimited" locale="nl"></configuration>
            </configurationGroup>
            <configurationGroup name="comments">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen bij positie</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="interview_comments">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Opmerkingen bij sollicitatiegesprek</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>