<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ManageRegistration" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ManageRegistration" path="/I2CE" locale="sw">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="sw">
      <configuration name="person_can_view_child_form_registration" locale="sw">
        <value>Weza kuangalia fomu ya usajili ya mtoto wa mtu</value>
      </configuration>
      <configuration name="person_can_edit_child_form_registration" locale="sw">
        <value>Weza kuhariri fomu ya usajili ya mtoto wa mtu</value>
      </configuration>
      <configuration name="can_edit_database_list_council" locale="sw">
        <value>Weza kuhariri orodha ya hazinadata ya baraza</value>
      </configuration>
      <configuration name="can_view_database_list_council" locale="sw">
        <value>Weza kuangalia orodha ya hazinadata ya baraza</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="registration">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="sw">
            <value>Ongeza/Sasisha Usajili</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="council">
          <configuration name="display" values="single" locale="sw">
            <value>Baraza</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registration">
          <configuration name="display" values="single" locale="sw">
            <value>Usajili</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Registration">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Baraza la Usajili</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Namba ya Usajili</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Namba ya Leseni</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Tarehe ya Usajili</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="headers" type="delimited" locale="sw">
                <value>default:Tarehe ya Kuisha kutumika Leseni</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>