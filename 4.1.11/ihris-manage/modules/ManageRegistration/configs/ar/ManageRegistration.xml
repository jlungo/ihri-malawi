<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ManageRegistration" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ManageRegistration" path="/I2CE" locale="ar">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ar">
      <configuration name="person_can_view_child_form_registration" locale="ar">
        <value>يمكن عرض نموذج التسجيل&#160;الفرعي&#160;لشخص</value>
      </configuration>
      <configuration name="person_can_edit_child_form_registration" locale="ar">
        <value>يمكن تعديل&#160;نموذج التسجيل&#160;الفرعي&#160;لشخص</value>
      </configuration>
      <configuration name="can_edit_database_list_council" locale="ar">
        <value>يمكن&#160;تعديل قائمة&#160;قاعدة بيانات&#160;المجلس</value>
      </configuration>
      <configuration name="can_view_database_list_council" locale="ar">
        <value>يمكن عرض&#160;قائمة&#160;قاعدة بيانات&#160;المجلس</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="registration">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>إضافة / تحديث&#160;التسجيل</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="council">
          <configuration name="display" values="single" locale="ar">
            <value>مجلس</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registration">
          <configuration name="display" values="single" locale="ar">
            <value>تسجيل</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Registration">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:مجلس التسجيل</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:رقم التسجيل</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:رقم الترخيص</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:تاريخ التسجيل</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:تاريخ انتهاء صلاحية الترخيص</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>