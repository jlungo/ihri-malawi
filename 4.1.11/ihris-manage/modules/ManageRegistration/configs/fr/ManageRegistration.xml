<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ManageRegistration" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ManageRegistration" path="/I2CE" locale="fr">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr">
      <configuration name="person_can_view_child_form_registration" locale="fr">
        <value>Peut afficher le formulaire enfant de l’enregistrement d’une personne</value>
      </configuration>
      <configuration name="person_can_edit_child_form_registration" locale="fr">
        <value>Peut éditer les formulaires enfants de l’enregistrement d’une personne</value>
      </configuration>
      <configuration name="can_edit_database_list_council" locale="fr">
        <value>Peut éditer la liste de base de données du Conseil</value>
      </configuration>
      <configuration name="can_view_database_list_council" locale="fr">
        <value>Peut afficher la liste de base de données du Conseil</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="registration">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr">
            <value>Ajouter/Actualiser un enregistrement</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="council">
          <configuration name="display" values="single" locale="fr">
            <value>Conseil</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registration">
          <configuration name="display" values="single" locale="fr">
            <value>Enregistrement</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Registration">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Conseil de l’ordre</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Numéro d’enregistrement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Numéro de licence</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date d’enregistrement</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="headers" type="delimited" locale="fr">
                <value>default:Date d’expiration de la license</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>