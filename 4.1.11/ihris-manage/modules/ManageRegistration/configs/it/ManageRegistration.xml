<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ManageRegistration" path="/I2CE">
  <metadata>
    <version>4.1.0.1</version>
  </metadata>
  <configurationGroup name="ManageRegistration" path="/I2CE" locale="it">
    <version>4.1.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="it">
      <configuration name="person_can_view_child_form_registration" locale="it">
        <value>Visualizza il sottomodulo di registrazione di una persona</value>
      </configuration>
      <configuration name="person_can_edit_child_form_registration" locale="it">
        <value>Modifica il sottomodulo di registrazione di una persona</value>
      </configuration>
      <configuration name="can_edit_database_list_council" locale="it">
        <value>Modifica l'lenco delle categorie professionali</value>
      </configuration>
      <configuration name="can_view_database_list_council" locale="it">
        <value>Visualizza l'elenco delle categorie professionali</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="registration">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="it">
            <value>Aggiungi/aggiorna registrazione</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="council">
          <configuration name="display" values="single" locale="it">
            <value>Categoria professionale</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="registration">
          <configuration name="display" values="single" locale="it">
            <value>Registrazione</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_Registration">
          <configurationGroup name="fields">
            <configurationGroup name="council">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Categoria professionale</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_number">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Numero registrazione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_number">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Numero licenza</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="registration_date">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Data di registrazione</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="license_expiration">
              <configuration name="headers" type="delimited" locale="it">
                <value>default:Data scadenza licenza</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>