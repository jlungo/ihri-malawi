<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.1.7.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="ar">
    <version>4.1.7.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="ar">
      <configuration name="username" path="user/username/unique"></configuration>
      <configuration name="pass_match" path="user/password/mismatch"></configuration>
      <configuration name="pass_match" path="user/password/required"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="ar">
      <configuration name="users_can_edit" locale="ar">
        <value>يمكن&#160;إنشاء مستخدم&#160;على النظام</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="ar">
        <value>يمكن تعديل&#160;كافة المستخدمين&#160;في النظام.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="ar">
        <value>يمكن&#160;إنشاء أو تعديل&#160;المستخدمين&#160;في النظام.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>إدارة المستخدمين&#160;/&#160;وصول</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="ar">
            <value>إدارة المستخدمين&#160;/&#160;وصول</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="ar">
          <value>دور</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:دور</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:ويمكن&#160;تخصيصه للمستخدم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="ar"></configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:سلسل</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:اسم المستخدم</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="ar">
                <value>default:كلمة المرور&#160;(اترك فارغا&#160;للحفاظ على&#160;نفس كلمة المرور)</value>
                <value>generate:إنتاج كلمة مرور جديدة</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="ar">
                <value>default:دور</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>