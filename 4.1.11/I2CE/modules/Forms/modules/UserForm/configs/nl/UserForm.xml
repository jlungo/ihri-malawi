<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.1.7.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="nl">
    <version>4.1.7.0.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="nl">
      <configuration name="username" path="user/username/unique">
        <value>Deze gebruikersnaam wordt al gebruikt.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/mismatch">
        <value>Het wachtwoord en het bevestigingswachtwoord zijn niet hetzelfde.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/required">
        <value>Voor nieuwe gebruikers moet een wachtwoord worden opgegeven.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="nl">
      <configuration name="users_can_edit" locale="nl">
        <value>Kan een gebruiker aanmaken in het systeem</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="nl">
        <value>Kan alle gebruikers in het systeem bewerken.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="nl">
        <value>Kan gebruikers in het systeem aanmaken of bewerken</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Gebruikers en toegang beheren</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="nl">
            <value>Gebruikers en toegang beheren</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="nl">
          <value>Rol</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Rol</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Kan toewijzen aan gebruiker</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Voorkeursstartpagina</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Omhoog druppelen</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Gebruikersnaam</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="nl">
                <value>default:Wachtwoord (leeg laten om het wachtwoord niet te wijzigen)</value>
                <value>generate:Nieuw wachtwoord genereren</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="nl">
                <value>default:Rol</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>