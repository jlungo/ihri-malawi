<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="web-services" path="/modules/web-services">
  <metadata>
    <version>4.1.10.0.1</version>
  </metadata>
  <configurationGroup name="web-services" path="/modules/web-services" locale="es">
    <version>4.1.9.0.1</version>
    <configurationGroup name="messages" locale="es">
      <configuration name="unknown_error" values="single">
        <value>Ocurrió un error desconocido.</value>
      </configuration>
      <configuration name="noaccess" values="single">
        <value>No tienes acceso a este servicio. Inicia sesión primero o contacta con el administrador.</value>
      </configuration>
      <configuration name="noaccess_page" values="single">
        <value>No tienes acceso al servicio '%s'.</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>