<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="RequestAccount-VerifyEmail">
  <metadata>
    <version>4.1.3.0.1</version>
  </metadata>
  <configurationGroup name="RequestAccount-VerifyEmail" locale="it">
    <version>4.1.3.0.1</version>
    <configurationGroup name="user_request_class_data" path="/modules/forms/formClasses/I2CE_User_Request">
      <configurationGroup name="fields">
        <configurationGroup name="request_number">
          <configuration name="headers" type="delimited" locale="it"></configuration>
        </configurationGroup>
        <configurationGroup name="verified">
          <configuration name="headers" type="delimited" locale="it"></configuration>
        </configurationGroup>
        <configurationGroup name="email">
          <configuration name="headers" type="delimited" locale="it"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="user_messages" locale="it">
      <configuration name="verify_email"></configuration>
      <configuration name="verify_email_fail"></configuration>
      <configuration name="verified"></configuration>
      <configuration name="not_verified">
        <value>Non abbiamo potuto verificare il tuo account.</value>
      </configuration>
      <configuration name="invalid_username"></configuration>
      <configuration name="invalid_email" path="/modules/forms/formfield_messages/user_request/email/invalid_email"></configuration>
      <configuration name="email_subject"></configuration>
      <configuration name="email_message"></configuration>
      <configuration name="email_message_verify_only"></configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="home">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="it"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>