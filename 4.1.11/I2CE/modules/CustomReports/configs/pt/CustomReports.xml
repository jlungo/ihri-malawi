<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports">
  <metadata>
    <version>4.1.11.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports" locale="pt">
    <version>4.1.9.0.1</version>
    <configurationGroup name="tasks" path="/modules/CustomReports/text/" locale="pt">
      <configuration name="Increasing">
        <value>Aumentar</value>
      </configuration>
      <configuration name="Decreasing">
        <value>Diminuir</value>
      </configuration>
      <configurationGroup name="user_messages">
        <configuration name="no_report_view"></configuration>
        <configuration name="invalid_report_view"></configuration>
        <configuration name="no_permission_report_view"></configuration>
        <configuration name="no_display_report_view"></configuration>
        <configuration name="reportView_merged_field_display">
          <value>%s de %s</value>
        </configuration>
        <configuration name="merge_display">
          <value>%s vinculado em %s</value>
        </configuration>
        <configurationGroup name="merge_descriptions">
          <configuration name="parent"></configuration>
          <configuration name="child"></configuration>
          <configuration name="default">
            <value>%s vinculado em %s</value>
          </configuration>
        </configurationGroup>
        <configuration name="report_no_name"></configuration>
        <configuration name="report_bad_name"></configuration>
        <configuration name="report_bad_used"></configuration>
        <configuration name="report_no_relationship"></configuration>
        <configuration name="report_bad_relationship"></configuration>
        <configuration name="cannot_delete"></configuration>
        <configuration name="bad_delete_path"></configuration>
        <configuration name="not_cached"></configuration>
        <configuration name="not_saved"></configuration>
        <configuration name="saved"></configuration>
        <configuration name="not_generated"></configuration>
        <configuration name="not_updated"></configuration>
        <configuration name="updated"></configuration>
      </configurationGroup>
      <configurationGroup name="report_status">
        <configuration name="does_not_exist"></configuration>
        <configuration name="not_generated"></configuration>
        <configuration name="stale"></configuration>
        <configuration name="failed">
          <value>Falhado</value>
        </configuration>
        <configuration name="in_progress">
          <value>Em progresso</value>
        </configuration>
        <configuration name="generated">
          <value>Gerado</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="headers">
        <configuration name="sum">
          <value>Soma</value>
        </configuration>
        <configuration name="maximum">
          <value>Máximo</value>
        </configuration>
        <configuration name="minimum">
          <value>Mínimo</value>
        </configuration>
        <configuration name="average">
          <value>Média</value>
        </configuration>
        <configuration name="count"></configuration>
        <configuration name="count_distinct">
          <value>Total Distinto</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt">
      <configuration name="custom_reports_can_access" locale="pt"></configuration>
      <configuration name="custom_reports_delete" locale="pt">
        <value>Permite a eliminação de dados sobre relatórios customizados</value>
      </configuration>
      <configuration name="custom_reports_can_access_relationships" locale="pt">
        <value>Permite acesso às Relações de Reporte Customizado</value>
      </configuration>
      <configuration name="custom_reports_can_edit_relationships" locale="pt">
        <value>Permite a edição de Relações de Relatório Customizado</value>
      </configuration>
      <configuration name="custom_reports_can_access_reports" locale="pt">
        <value>Permite Acesso a Relatórios Customizados</value>
      </configuration>
      <configuration name="custom_reports_can_edit_reports" locale="pt"></configuration>
      <configuration name="custom_reports_can_view_reportViews" locale="pt">
        <value>Permite a Visualização de Relatórios Customizados</value>
      </configuration>
      <configuration name="custom_reports_can_edit_reportViews" locale="pt">
        <value>Permite a edição da Visualização de Relatórios Customizados</value>
      </configuration>
      <configuration name="custom_reports_admin" locale="pt">
        <value>Administrador de relatórios customizados. Pode realizar todas as tarefas associadas aos relatórios customizados</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="command_line">
      <configurationGroup name="show">
        <configurationGroup name="args">
          <configuration name="title" locale="pt">
            <value>Ver Relatórios</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="page_styles" path="/I2CE/template/page_styles">
        <configurationGroup name="customreport_config">
          <configurationGroup name="args">
            <configuration name="title" locale="pt">
              <value>Criar Relatórios</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="show">
        <configurationGroup name="args">
          <configuration name="title" locale="pt">
            <value>Ver Relatórios</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="saveOptions">
        <configurationGroup name="args">
          <configuration name="title" locale="pt">
            <value>Ver Relatórios</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="nodisplay">
        <configurationGroup name="args">
          <configuration name="title" locale="pt"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>