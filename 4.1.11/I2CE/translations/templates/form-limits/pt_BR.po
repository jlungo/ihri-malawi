#, no-c-format
msgid "Limit this field to be between the following values"
msgstr "Limitar este campo para que fique entre os seguintes valores"

#, no-c-format
msgid "Limit this field to contain (case insensitive) the following value"
msgstr ""
"Limitar este campo para conter (não diferencia maiúsculas/minúsculas) o "
"seguinte valor"

#, no-c-format
msgid "Limit this field to be Yes or No"
msgstr "Limitar este campo em Sim ou Não"

#, no-c-format
msgid "Select Value"
msgstr "Selecione Valor"

#, no-c-format
msgid "No"
msgstr "Não"

#, no-c-format
msgid "Yes"
msgstr "Sim"

#, no-c-format
msgid "Limit this field to be greather than the current date."
msgstr "Limitar este campo para que seja maior que a data atual."

#, no-c-format
msgid "Limit this field to be not null (value specified)."
msgstr "Limitar este campo para que seja não-nulo (valor especificado)."

#, no-c-format
msgid "Limit this field to be less than or equal to the following value"
msgstr "Limitar este campo para que seja menor ou igual que o valor seguinte"

#, no-c-format
msgid "Limit this field to be like (case insensitive) the following value"
msgstr ""
"Limitar este campo para seja similar (não diferencia maiúsculas/minúsculas) "
"ao valor seguinte"

#, no-c-format
msgid "Limit this field to be True or False"
msgstr "Limitar este campo em Verdadeiro ou Falso"

#, no-c-format
msgid "True"
msgstr "Verdadeiro"

#, no-c-format
msgid "False"
msgstr "Falso"

#, no-c-format
msgid "Limit this field to be equal to the given value."
msgstr "Limitar este campo para que seja igual ao valor dado."

#, no-c-format
msgid ""
"Limit this field to be the minimum for all child forms that have the same "
"parent."
msgstr ""

#, no-c-format
msgid "Limit this field to be like (case sensitive) the following value"
msgstr ""
"Limitar este campo para que seja (diferencia maiúscula/minúscula) como o "
"seguinte valor"

#, no-c-format
msgid "Limit this field to be equal to the following value"
msgstr "Limitar este campo para que seja igual ao seguinte valor"

#, no-c-format
msgid "Limit this field to be null or not"
msgstr "Limitar este campo para que seja nulo ou não"

#, no-c-format
msgid "Limit this field to be no."
msgstr "Limitar este campo para que seja não."

#, no-c-format
msgid ""
"Limit this field to be the minimum value for this form having the same "
"parent."
msgstr ""
"Limitar este campo para que seja o menor valor desse formulário contendo o "
"mesmo pai."

#, no-c-format
msgid "Limit this field to be greater than or equal to the following value"
msgstr ""
"Limitar este campo para que seja maior que ou igual ao seguinte valor"

#, no-c-format
msgid "Limit this field to be true."
msgstr "Limitar este campo para que seja verdadeiro."

#, no-c-format
msgid ""
"Limit this field to be equal to (case insensitive) the following value"
msgstr ""
"Limitar este campo para que seja (não diferencia maiúscula/minúscula) o "
"seguinte valor"

#, no-c-format
msgid ""
"Limit this field to be the maximum for all child forms that have the same "
"parent."
msgstr ""
"Limitar este campo para que seja o valor máximo dentre todos os fomulários "
"filhos que contenham o mesmo formulário pai."

#, no-c-format
msgid "Limit this field to be greater than the following value"
msgstr "Limitar este campo para que seja maior que o seguinte valor"

#, no-c-format
msgid "Limit this field to be one of the following values"
msgstr "Limitar este campo para que seja um dos seguintes valores"

#, no-c-format
msgid "Limit this field to be less than the current date."
msgstr "Limitar este campo para que seja menor que a data atual."

#, no-c-format
msgid ""
"Limit this field to be the maximum value for this form having the same "
"parent."
msgstr ""
"Limitar este campo para que seja o valor máximo desse formulário contendo o "
"mesmo formulário pai."

#, no-c-format
msgid "Limit this field to be less than the following value"
msgstr "Limitar este campo para que seja menor que o seguinte valor"

#, no-c-format
msgid ""
"Limit this field to be one of the following values: (Hold down the Control "
"key to select multiple values)"
msgstr ""
"Limitar este campo para que seja um dos seguintes valores: (Pressione e "
"segure a tecla CTRL para selecionar múltiplos valores)"

#, no-c-format
msgid "Limit this field to be yes."
msgstr "Limitar este campo para que seja sim."

#, no-c-format
msgid "Limit this field to be false."
msgstr "Limitar este campo para que seja falso."

#, no-c-format
msgid "Limit this field to be null (value not specified)."
msgstr "Limitar este campo para que seja nulo (valor não especificado)."
