#, no-c-format
msgid "Limit this field to be between the following values"
msgstr "Omezit toto pole, aby bylo mezi následujícími hodnotami"

#, no-c-format
msgid "Limit this field to contain (case insensitive) the following value"
msgstr ""
"Omezit toto pole, aby obsahovalo (nerozlišuje velikost písmen) následující "
"hodnotu"

#, no-c-format
msgid "Limit this field to be Yes or No"
msgstr "Omezit toto pole na Ano nebo Ne"

#, no-c-format
msgid "Select Value"
msgstr "Vyberte Hodnotu"

#, no-c-format
msgid "No"
msgstr "Ne"

#, no-c-format
msgid "Yes"
msgstr "Ano"

#, no-c-format
msgid "Limit this field to be greather than the current date."
msgstr "Omezit toto pole, aby bylo vyšší než současné datum."

#, no-c-format
msgid "Limit this field to be not null (value specified)."
msgstr "Omezit toto pole, aby nebylo prázdné (hodnota zadána)."

#, no-c-format
msgid "Limit this field to be less than or equal to the following value"
msgstr "Omezit toto pole, aby bylo menší nebo rovno následující hodnotě"

#, no-c-format
msgid "Limit this field to be like (case insensitive) the following value"
msgstr ""
"Omezit toto pole, aby bylo jako (nerozlišuje velikost písmen) následující "
"hodnota"

#, no-c-format
msgid "Limit this field to be True or False"
msgstr "Omezit toto pole na Pravda nebo Nepravda"

#, no-c-format
msgid "True"
msgstr "Pravda"

#, no-c-format
msgid "False"
msgstr "Nepravda"

#, no-c-format
msgid "Limit this field to be equal to the given value."
msgstr "Omezit toto pole, aby bylo rovno zadané hodnotě."

#, no-c-format
msgid ""
"Limit this field to be the minimum for all child forms that have the same "
"parent."
msgstr ""
"Omezit toto pole na minimum všech podřazených formulářů, které mají stejný "
"nadřazený formulář."

#, no-c-format
msgid "Limit this field to be like (case sensitive) the following value"
msgstr ""
"Omezit toto pole, aby bylo jako (rozlišuje velikost písmen) následující "
"hodnota"

#, no-c-format
msgid "Limit this field to be equal to the following value"
msgstr "Omezit toto pole, aby bylo rovno následující hodnotě"

#, no-c-format
msgid "Limit this field to be null or not"
msgstr "Omezit toto pole, aby bylo nebo nebylo prázdné"

#, no-c-format
msgid "Limit this field to be no."
msgstr "Omezit toto pole na ne."

#, no-c-format
msgid ""
"Limit this field to be the minimum value for this form having the same "
"parent."
msgstr ""
"Omezit toto pole na minimální hodnotu tohoto formuláře, mající stejný "
"nadřazený formulář."

#, no-c-format
msgid "Limit this field to be greater than or equal to the following value"
msgstr "Omezit toto pole, aby bylo větší nebo rovno zadané hodnotě"

#, no-c-format
msgid "Limit this field to be true."
msgstr "Omezit toto pole na pravda"

#, no-c-format
msgid ""
"Limit this field to be equal to (case insensitive) the following value"
msgstr ""
"Omezit toto pole, aby bylo rovno (nerozlišuje velikost písmen) následující "
"hodnotě"

#, no-c-format
msgid ""
"Limit this field to be the maximum for all child forms that have the same "
"parent."
msgstr ""
"Omezit toto pole na maximum všch podřazených formulářů, které mají stejný "
"nadřazený formulář."

#, no-c-format
msgid "Limit this field to be greater than the following value"
msgstr "Omezit toto pole, aby bylo větší než následující hodnota"

#, no-c-format
msgid "Limit this field to be one of the following values"
msgstr "Omezit toto pole na jednu z následujících hodnot"

#, no-c-format
msgid "Limit this field to be less than the current date."
msgstr "Omezit toto pole, aby bylo menší než současné datum"

#, no-c-format
msgid ""
"Limit this field to be the maximum value for this form having the same "
"parent."
msgstr ""
"Omezit toto pole na maximální hodnotu tohoto formuláře, mající stejný "
"nadřazený formulář."

#, no-c-format
msgid "Limit this field to be less than the following value"
msgstr "Omezit toto pole, aby bylo menší než následující hodnota"

#, no-c-format
msgid ""
"Limit this field to be one of the following values: (Hold down the Control "
"key to select multiple values)"
msgstr ""
"Omezit toto pole na jednu z následujících hodnot: (Držte klávesu Ctrl pro "
"vybrání více hodnot)"

#, no-c-format
msgid "Limit this field to be yes."
msgstr "Omezit toto pole na ano."

#, no-c-format
msgid "Limit this field to be false."
msgstr "Omezit toto pole na nepravda."

#, no-c-format
msgid "Limit this field to be null (value not specified)."
msgstr "Omezit toto pole, aby bylo prázdné (hodnota nezadána)."
