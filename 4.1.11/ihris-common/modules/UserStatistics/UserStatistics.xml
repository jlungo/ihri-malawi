<?xml version="1.0"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserStatistics">
  <metadata>
    <displayName>User Statistics</displayName>
    <className>iHRIS_Module_UserStatistics</className>
    <category>Administration</category>
    <description>User Statistics page and updates for user view page.</description>
    <creator>Intrahealth Informatics</creator>
    <email>hris@capacityproject.org</email>
    <link>https://launchpad.net/ihris-common</link>
    <version>4.1.10.0</version>
    <path name="configs">
      <value>./configs</value>
    </path>
    <requirement name="UserAccess">
      <atLeast version="4.1" />
      <lessThan version="4.2" />
    </requirement>
    <requirement name="UserForm">
      <atLeast version="4.1" />
      <lessThan version="4.2" />
    </requirement>
    <requirement name="forms-storage-entry">
      <atLeast version="4.1" />
      <lessThan version="4.2" />
    </requirement>
    <requirement name="CustomReports">
      <atLeast version="4.1" />
      <lessThan version="4.2" />
    </requirement>
    <path name="classes">
      <value>./lib</value>
    </path>
    <path name="templates">
      <value>./templates</value>
    </path>
    <path name="css">
      <value>./css</value>
    </path>
    <priority>150</priority>
  </metadata>
  <erase path="/modules/UserStatistics/display/change_type">
    <lessThan version="4.1.5.19" />
  </erase>
  <configurationGroup name="UserStatistics" path="/modules/UserStatistics">
    <displayName>User Statistics</displayName>
    <configurationGroup name="task_description" path="/I2CE/tasks/task_description">
      <version>4.1.9.2</version>
      <configuration name="can_view_user_statistics" locale="en_US">
        <value>Can view the user statistics page.</value>
      </configuration>
    </configurationGroup>
    <configuration name="form_change_type" path="/modules/forms/forms/entry_change_type" type="delimited">
      <displayName>Entry Change Type</displayName>
      <version>4.1.5.19</version>
      <value>class:I2CE_SimpleList</value>
      <value>display:Entry Change Type</value>
      <value>storage:magicdata</value>
    </configuration>
    <configurationGroup name="formsData_change_type" path="/I2CE/formsData/forms/entry_change_type">
      <displayName>Entry Change Type</displayName>
      <version>4.1.5.19</version>
      <configurationGroup name="1">
        <configuration name="last_modified">
          <value>2013-02-28 09:17:00</value>
        </configuration>
        <configuration name="fields" value="many" type="delimited">
          <value>name:Initial Entry</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="2">
        <configuration name="last_modified">
          <value>2013-02-28 09:17:00</value>
        </configuration>
        <configuration name="fields" value="many" type="delimited">
          <value>name:Verified Value</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="3">
        <configuration name="last_modified">
          <value>2013-02-28 09:17:00</value>
        </configuration>
        <configuration name="fields" value="many" type="delimited">
          <value>name:Corrected Value</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="4">
        <configuration name="last_modified">
          <value>2013-02-28 09:17:00</value>
        </configuration>
        <configuration name="fields" value="many" type="delimited">
          <value>name:Updated Value</value>
        </configuration>
      </configurationGroup>
    </configurationGroup>
    <!-- end formsData_change_type -->
    <configuration name="display_name" locale="en_US">
      <version>4.1.5.2</version>
      <value>User Statistics</value>
    </configuration>
    <configuration name="description" locale="en_US">
      <version>4.1.5.2</version>
      <value>List of all changes made to individual form fields for all users.</value>
    </configuration>
    <configurationGroup name="default_display_options">
      <version>4.1.5.8</version>
      <configuration name="sort_order">
        <value>-date</value>
      </configuration>
    </configurationGroup>
    <configuration name="display_order">
      <version>4.1.9.2</version>
      <value>change_type,date,parent_id,record,form,field,value,user,username</value>
    </configuration>
    <configurationGroup name="fields">
      <version>4.1.5.17</version>
      <configurationGroup name="date">
        <configuration name="db_field">
          <value>entry.date</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Date Modified</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configuration name="formfield">
          <value>DATE_YMD</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="greaterthan_equals">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Date Modified On or After</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="lessthan_equals">
            <version>4.1.9.1</version>
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Date Modified On or Before</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="username">
        <configuration name="db_field">
          <value>user.username</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Username</value>
        </configuration>
        <configuration name="link">
          <value>view_user?username=</value>
        </configuration>
        <configuration name="link_append">
          <value>username</value>
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configuration name="formfield">
          <value>MAP</value>
        </configuration>
        <configuration name="select_forms">
          <value>user</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Username</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="user">
        <configuration name="db_field">
          <value>IF(entry.who = 0, 'I2CE Admin', CONCAT_WS( ' ', user.firstname, user.lastname ) )</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>User</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="form">
        <configuration name="db_field">
          <value>form.name</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Form</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configuration name="formfield">
          <value>STRING_LINE</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Form</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="field">
        <configuration name="db_field">
          <value>field.name</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Field</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="record">
        <configuration name="db_field">
          <value>entry.record</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Record ID</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configuration name="formfield">
          <value>INT</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Record ID</value>
            </configuration>
          </configurationGroup>
          <configurationGroup name="null_not_null">
            <version>4.1.9.3</version>
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Display All Fields</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="parent_id">
        <version>4.1.9.2</version>
        <configuration name="db_field">
          <value>CONCAT(record.parent_form,'|',record.parent_id)</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Parent ID</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configuration name="formfield">
          <value>STRING_LINE</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Parent ID</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="value">
        <configuration name="db_field" values="many">
          <version>4.1.5.18</version>
          <value>entry.string_value</value>
          <value>entry.integer_value</value>
          <value>entry.date_value</value>
          <value>entry.text_value</value>
          <value>entry.blob_value</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Value</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configurationGroup name="limits">
          <version>4.1.9.1</version>
          <configurationGroup name="contains">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Value Search</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="change_type">
        <version>4.1.5.19</version>
        <configuration name="db_field">
          <value>entry.change_type</value>
        </configuration>
        <configuration name="header" locale="en_US">
          <value>Change Type</value>
        </configuration>
        <configuration name="link">
          <value />
        </configuration>
        <configuration name="link_append">
          <value />
        </configuration>
        <configuration name="link_type">
          <value>link</value>
        </configuration>
        <configuration name="formfield">
          <value>MAP</value>
        </configuration>
        <configuration name="select_forms">
          <value>entry_change_type</value>
        </configuration>
        <configurationGroup name="limits">
          <configurationGroup name="equals">
            <configuration name="enabled">
              <value>1</value>
            </configuration>
            <configuration name="header" locale="en_US">
              <value>Change Type</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="defaults">
      <version>4.1.4.1</version>
      <configuration name="login_limit">
        <displayName>Number of login entries to display on the user page.</displayName>
        <value>10</value>
      </configuration>
      <configuration name="days_forms_limit">
        <displayName>Number of days to display on user page for all forms that have been edited.</displayName>
        <value>5</value>
      </configuration>
      <configuration name="days_person_limit">
        <displayName>Number of days to display on user page for the person and child count display.</displayName>
        <value>5</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <version>4.1.4.1</version>
      <displayName>Pages</displayName>
      <description>Information about various pages made available by the system</description>
      <status>required:true</status>
      <configurationGroup name="home">
        <displayName>User Statistics</displayName>
        <description>The page 'home' which has the action of: User Statistics</description>
        <configuration name="style" values="single">
          <displayName>Page Style</displayName>
          <description>The Page Style</description>
          <value>shell</value>
        </configuration>
        <configuration name="class" values="single">
          <displayName>Page Class</displayName>
          <description>The class responsible for displaying this page</description>
          <status>required:true</status>
          <value>iHRIS_PageUserStatistics</value>
        </configuration>
        <configurationGroup name="args">
          <displayName>Page Options</displayName>
          <description>The options that control the access and display of all pages</description>
          <configuration name="title" values="single" locale="en_US">
            <displayName>Page Title</displayName>
            <description>Page Title</description>
            <status>required:true</status>
            <value>User Statistics</value>
          </configuration>
          <configuration name="tasks" values="many">
            <version>4.1.9.2</version>
            <value>can_view_user_statistics</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>